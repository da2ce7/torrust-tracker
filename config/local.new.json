{
  "global": {
    "log_filter_level": "Info",
    "external_ip": "0.0.0.0",
    "is_on_reverse_proxy": false
  },
  "common": {
    "tracker_mode": "public",
    "announce_interval_seconds": 120,
    "announce_interval_seconds_minimum": null,
    "peer_timeout_seconds_maximum": 900,
    "enable_tracker_usage_statistics": true,
    "enable_persistent_statistics": false,
    "cleanup_inactive_peers_interval_seconds": 600,
    "enable_peerless_torrent_pruning": true
  },
  "database": {
    "driver": "Sqlite3",
    "path": "data.db"
  },
  "service": {
    "api_imported_0": {
      "enabled": true,
      "display_name": "HTTP API (imported)",
      "service": "API",
      "socket": "127.0.0.1:1212",
      "tls": null,
      "access_tokens": {
        "admin": "MyAccessToken"
      }
    },
    "tls_imported_0": {
      "enabled": true,
      "display_name": "TLS Service (imported)",
      "service": "TLS",
      "socket": "0.0.0.0:6969",
      "tls": {
        "certificate_file_path": "",
        "key_file_path": ""
      },
      "access_tokens": null
    },
    "udp_imported_0": {
      "enabled": true,
      "display_name": "UDP Service (imported)",
      "service": "UDP",
      "socket": "0.0.0.0:6969",
      "tls": null,
      "access_tokens": null
    }
  }
}