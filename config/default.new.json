{
  "global": {
    "log_filter_level": "Info",
    "external_ip": "",
    "on_reverse_proxy": false
  },
  "common": {
    "tracker_mode": "listed",
    "database": {
      "driver": "Sqlite3",
      "path": "data.db"
    },
    "announce_interval": 120,
    "min_announce_interval": 120,
    "max_peer_timeout": 900,
    "tracker_usage_statistics": true,
    "persistent_torrent_completed_stat": false,
    "inactive_peer_cleanup_interval": 600,
    "remove_peerless_torrents": false
  },
  "service": {
    "default_api": {
      "enabled": false,
      "display_name": "HTTP API (default)",
      "service": "API",
      "socket": "127.0.0.1:1212",
      "tls": null,
      "access_tokens": {
        "admin": "password"
      }
    },
    "default_http": {
      "enabled": false,
      "display_name": "HTTP (default)",
      "service": "HTTP",
      "socket": "0.0.0.0:6969",
      "tls": null,
      "access_tokens": null
    },
    "default_tls": {
      "enabled": false,
      "display_name": "TLS (default)",
      "service": "HTTP",
      "socket": "0.0.0.0:6969",
      "tls": {
        "certificate_file_path": "",
        "key_file_path": ""
      },
      "access_tokens": null
    },
    "default_udp": {
      "enabled": false,
      "display_name": "UDP (default)",
      "service": "UDP",
      "socket": "0.0.0.0:6969",
      "tls": null,
      "access_tokens": null
    }
  }
}