{
  "global": {
    "log_filter_level": "Info",
    "external_ip": "",
    "is_on_reverse_proxy": false
  },
  "common": {
    "tracker_mode": "listed",
    "announce_interval_seconds": 120,
    "announce_interval_seconds_minimum": 120,
    "peer_timeout_seconds_maximum": 900,
    "enable_tracker_usage_statistics": true,
    "enable_persistent_statistics": false,
    "cleanup_inactive_peers_interval_seconds": 600,
    "enable_peerless_torrent_pruning": false
  },
  "database": {
    "driver": "Sqlite3",
    "path": "data.db"
  },
  "service": {
    "default_api": {
      "enabled": false,
      "display_name": "HTTP API (default)",
      "service": "API",
      "socket": "127.0.0.1:1212",
      "tls": null,
      "access_tokens": {
        "admin": "password"
      }
    },
    "default_http": {
      "enabled": false,
      "display_name": "HTTP (default)",
      "service": "HTTP",
      "socket": "0.0.0.0:6969",
      "tls": null,
      "access_tokens": null
    },
    "default_tls": {
      "enabled": false,
      "display_name": "TLS (default)",
      "service": "HTTP",
      "socket": "0.0.0.0:6969",
      "tls": {
        "certificate_file_path": "",
        "key_file_path": ""
      },
      "access_tokens": null
    },
    "default_udp": {
      "enabled": false,
      "display_name": "UDP (default)",
      "service": "UDP",
      "socket": "0.0.0.0:6969",
      "tls": null,
      "access_tokens": null
    }
  }
}